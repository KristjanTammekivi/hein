{
  "name": "hein",
  "version": "1.5.0",
  "engines": {
    "node": ">=14.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/KristjanTammekivi/hein"
  },
  "homepage": "https://heinjs.com",
  "description": "Assertion library written in TypeScript",
  "keywords": [
    "typescript",
    "assertion",
    "assert",
    "test",
    "testing"
  ],
  "main": "dist/index.js",
  "exports": {
    ".": "./dist/index.js",
    "./assert": "./dist/assert.js",
    "./expect.types": "./dist/expect.types.js"
  },
  "typesVersions": {
    "*": {
      "assert": [
        "dist/assert"
      ],
      "expect.types": [
        "dist/expect.types"
      ]
    }
  },
  "scripts": {
    "build": "rimraf dist && tsc -b .",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "npm run lint -- --fix",
    "test": "node --trace-warnings node_modules/.bin/mocha 'src/**/*.test.ts'",
    "test:watch": "node --trace-warnings node_modules/.bin/mocha --watch 'src/**/*.test.ts'",
    "coverage": "nyc npm run test",
    "generate-docs": "typedoc --out docs src/index.ts"
  },
  "author": "Kristjan Tammekivi",
  "license": "MIT",
  "dependencies": {
    "hein-assertion-utils": "^1.0.4",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@types/lodash": "^4.17.0",
    "@types/mocha": "^10.0.7",
    "@types/mustache": "^4.2.1",
    "@types/node": "^22.9.1",
    "eslint": "^8.57.0",
    "eslint-config-hein": "^1.0.4",
    "mocha": "^10.7.0",
    "nyc": "^17.1.0",
    "rimraf": "^5.0.1",
    "source-map-support": "^0.5.21",
    "ts-node": "^10.9.1",
    "typedoc": "^0.26.7",
    "typescript": "^5.6.2"
  }
}
