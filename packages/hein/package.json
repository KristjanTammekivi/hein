{
  "name": "hein",
  "version": "1.0.1",
  "engines": {
    "node": ">=14.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/KristjanTammekivi/hein"
  },
  "description": "Assertion library written in TypeScript",
  "keywords": [
    "typescript",
    "assertion",
    "assert",
    "test"
  ],
  "main": "dist/index.js",
  "exports": {
    ".": "./dist/index.js",
    "./assert": "./dist/assert.js",
    "./expect.types": "./dist/expect.types.js"
  },
  "typesVersions": {
    "*": {
      "assert": [
        "dist/assert"
      ],
      "expect.types": [
        "dist/expect.types"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rimraf dist && tsc -b .",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "npm run lint -- --fix",
    "test": "node --trace-warnings node_modules/.bin/mocha 'src/**/*.test.ts'",
    "test:watch": "node --trace-warnings node_modules/.bin/mocha --watch 'src/**/*.test.ts'",
    "coverage": "nyc npm run test"
  },
  "author": "Kristjan Tammekivi",
  "license": "MIT",
  "dependencies": {
    "hein-assertion-utils": "^1.0.2",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.184",
    "@types/mocha": "^10.0.0",
    "@types/mustache": "^4.2.1",
    "@types/node": "^18.8.3",
    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "@typescript-eslint/parser": "^5.40.0",
    "eslint": "^8.24.0",
    "eslint-config-hein": "^1.0.0",
    "mocha": "^10.0.0",
    "nyc": "^15.1.0",
    "rimraf": "^3.0.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.2"
  }
}
