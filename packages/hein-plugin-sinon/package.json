{
  "name": "hein-plugin-sinon",
  "version": "1.0.0",
  "description": "hein plugin for sinon assertions",
  "main": "dist/index.js",
  "author": "Kristjan Tammekivi",
  "license": "MIT",
  "scripts": {
    "build": "tsc -b .",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "npm run lint -- --fix",
    "test": "node --trace-warnings node_modules/.bin/mocha 'src/**/*.test.ts'",
    "test:watch": "node --trace-warnings node_modules/.bin/mocha --watch 'src/**/*.test.ts'",
    "coverage": "nyc npm run test"
  },
  "dependencies": {
    "@types/sinon": "^10.0.13",
    "hein-assertion-utils": "*"
  },
  "devDependencies": {
    "eslint": "^8.22.0",
    "eslint-config-hein": "^1.0.0",
    "hein": "0.0.2",
    "mocha": "^10.0.0",
    "nyc": "^15.1.0",
    "sinon": "^14.0.0",
    "ts-node": "^10.9.1"
  }
}
